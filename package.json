{
  "name": "playrbase",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_PUBLIC_SURREAL_ENDPOINT=\"http://localhost:13001\" NEXT_PUBLIC_SURREAL_NAMESPACE=\"playrbase-deployment_local\" NEXT_PUBLIC_SURREAL_DATABASE=\"playrbase\" pnpm rawdev",
    "rawdev": "concurrently --names \"playrbase\" --hide 1 \"pnpm run dev-next-server\" \"surreal start --user root --pass root --bind 0.0.0.0:13001 file:dev.db\"",
    "dev-next-server": "cross-env NEXT_PUBLIC_ENV=\"dev\" next dev -p 13000",
    "ts": "tsc --watch",
    "lint": "eslint . --ext ts --ext tsx --max-warnings 0",
    "deploy": "export NEXT_PUBLIC_DEPLOYMENT_STATUS=\"deployed\" SURREAL_NAMESPACE=playrbase-deployment_$CF_PAGES_BRANCH && export NEXT_PUBLIC_SURREAL_NAMESPACE=$SURREAL_NAMESPACE && node ../cli/predeploy.js && pnpm build",
    "build": "next build && next export && next-sitemap && rm -rf out/dev"
  },
  "dependencies": {
    "@mui/material": "^5.11.8",
    "@svgr/webpack": "^6.5.1",
    "@tanstack/react-query": "^4.24.6",
    "@theopensource-company/awaited-surrealdb": "^0.5.1",
    "@types/node": "18.15.10",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "class-variance-authority": "^0.4.0",
    "eslint": "8.28.0",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1",
    "next": "13.0.4",
    "prettier-plugin-organize-imports": "^3.2.2",
    "ra-core": "^4.7.6",
    "ra-input-rich-text": "^4.7.6",
    "react": "18.2.0",
    "react-admin": "^4.7.6",
    "react-admin-json-view": "^2.0.0",
    "react-dom": "18.2.0",
    "react-feather": "^2.0.10",
    "react-i18next": "^12.1.5",
    "surrealdb.js": "^0.5.0",
    "tailwindcss": "^3.2.6",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@mui/icons-material": "^5.11.0",
    "@theopensource-company/surrealdb-cloudflare": "^1.0.4",
    "@tiptap/core": "2.0.0-beta.209",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-only-error": "^1.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "history": "^5.3.0",
    "node-fetch": "^3.3.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "ra-ui-materialui": "^4.7.6",
    "react-hook-form": "^7.43.1",
    "react-router": "^6.8.1",
    "react-router-dom": "^6.8.1"
  },
  "peerDependencies": {
    "@babel/core": "^7.20.7",
    "@mui/icons-material": "^5.11.0",
    "@tiptap/core": "2.0.0-beta.209",
    "history": "^5.3.0",
    "ra-ui-materialui": "^4.6.2",
    "react-hook-form": "^7.41.2",
