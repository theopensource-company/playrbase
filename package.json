{
  "name": "playrbase",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently --names \"playrbs,surreal\" --hide 1 \"pnpm dev-next\" \"pnpm dev-surreal\"",
    "debug": "concurrently --names \"playrbs,surreal\" \"pnpm dev-next\" \"pnpm dev-surreal\"",
    "dbonly": "concurrently --names \"playrbs,surreal\" --hide 0 \"pnpm dev-next\" \"pnpm dev-surreal\"",
    "dev-next": "cross-env NODE_ENV=\"development\" next dev -p 13000",
    "dev-surreal": "surreal start --log debug --user root --pass root --bind 0.0.0.0:13001 file:dev.db",
    "ts": "tsc --watch",
    "lint": "eslint . --ext ts --ext tsx --max-warnings 0",
    "deploy": "export NODE_ENV=\"production\" SURREAL_DATABASE=playrbase-deployment_$VERCEL_GIT_COMMIT_REF && export NEXT_PUBLIC_SURREAL_DATABASE=$SURREAL_DATABASE && ts-node ./scripts/predeploy.ts && pnpm build",
    "build": "next build && rm -rf out/dev"
  },
  "dependencies": {
    "@mui/material": "^5.12.3",
    "@svgr/webpack": "^8.0.0",
    "@tanstack/react-query": "^4.29.5",
    "@types/node": "20.2.4",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/react-modal": "^3.16.0",
    "class-variance-authority": "^0.6.0",
    "eslint": "8.40.0",
    "i18next": "^22.4.15",
    "i18next-browser-languagedetector": "^7.0.1",
    "next": "13.4.1",
    "prettier-plugin-organize-imports": "^3.2.2",
    "ra-core": "^4.10.2",
    "ra-input-rich-text": "^4.10.2",
    "react": "18.2.0",
    "react-admin": "^4.10.2",
    "react-admin-json-view": "^2.0.0",
    "react-dom": "18.2.0",
    "react-feather": "^2.0.10",
    "react-i18next": "^12.2.2",
    "react-modal": "^3.16.1",
    "surrealdb.js": "^0.7.1",
    "tailwindcss": "^3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@mui/icons-material": "^5.11.16",
    "@theopensource-company/surrealdb-cloudflare": "^1.0.4",
    "@tiptap/core": "2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint-config-next": "13.4.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-only-error": "^1.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "history": "^5.3.0",
    "node-fetch": "^3.3.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "ra-ui-materialui": "^4.10.2",
    "react-hook-form": "^7.43.9",
    "react-router": "^6.11.1",
    "react-router-dom": "^6.11.1",
    "ts-node": "^10.9.1"
  },
  "peerDependencies": {
    "@babel/core": "^7.20.7",
    "@mui/icons-material": "^5.11.0",
    "@tiptap/core": "2.0.0-beta.209",
    "history": "^5.3.0",
    "ra-ui-materialui": "^4.6.2",
    "react-hook-form": "^7.41.2",
    "react-router": "^6.6.1",
    "react-router-dom": "^6.6.1"
  }
}
