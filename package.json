    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "react-hook-form": "^7.51.2",
    "ts-node": "^10.9.2",
    "utf-8-validate": "^6.0.3"
  },
  "peerDependencies": {
    "@babel/core": "^7.20.7",
